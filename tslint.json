{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-sort-keys": [
            false
        ]
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    }
}
